{
  "expo": {
    "name": "Zyro AC",
    "slug": "zyro-ac",
    "version": "1.0.0",
    "icon": "./assets/logo.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "url": "https://u.expo.dev/30693a71-c30b-493d-a2b6-8c8727017f49",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
